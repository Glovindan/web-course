.container {

}

.inputSearch {
    border: none;
    color: rgba(0,0,0,0.5);
    margin: 10px;
    height: 24px;
    width: 400px;
    font-size: large;
    background: #efcef0;
    padding: 0 20px;
    box-shadow: inset 2px 2px 6px 0 rgba(0,0,0,0.5), inset -2px -2px 6px 0 rgba(255,255,255,0.83);
    border-radius: 20px;
    transition: 0.4s;
    
    &:focus {
        box-shadow: inset 2px 2px 6px 0 rgba(0,0,0,0), inset -2px -2px 6px 0 rgba(0,0,0,0);
    }
}